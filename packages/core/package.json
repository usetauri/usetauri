{
  "name": "@usetauri/core",
  "type": "module",
  "version": "0.0.1",
  "author": "Luca <https://github.com/lucacicada>",
  "homepage": "https://github.com/usetauri/usetauri#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usetauri/usetauri.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/usetauri/usetauri/issues"
  },
  "keywords": [
    "tauri",
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js"
    },
    "./*": "./*"
  },
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "**/*.d.ts",
    "**/*.js"
  ],
  "scripts": {
    "build": "tsc",
    "prepack": "pnpm run build"
  },
  "peerDependencies": {
    "@tauri-apps/api": "catalog:",
    "@tauri-apps/plugin-fs": "catalog:",
    "@tauri-apps/plugin-sql": "catalog:",
    "@tauri-apps/plugin-upload": "catalog:"
  },
  "peerDependenciesMeta": {
    "@tauri-apps/api": {
      "optional": true
    },
    "@tauri-apps/plugin-fs": {
      "optional": true
    },
    "@tauri-apps/plugin-sql": {
      "optional": true
    },
    "@tauri-apps/plugin-upload": {
      "optional": true
    }
  }
}
